{
    "name": "iq-180",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start:client": "ng serve",
        "test:client": "ng test",
        "lint:client": "ng lint",
        "build:stats": "ng build --stats-json",
        "analyze:client": "webpack-bundle-analyzer dist/client/stats-es2015.json",
        "e2e:client": "ng e2e",
        "serve": "node serve-script",
        "compile:server": "webpack --config webpack.server.config.js --progress --colors",
        "serve:ssr": "NODE_ENV=production node dist/server",
        "build:ssr": "yarn build:client-and-server-bundles && yarn compile:server",
        "build:client-and-server-bundles": "ng build --prod && ng run iq-one-eighty-frontend:server:production",
        "build": "yarn build:ssr",
        "format": "prettier --write \"src/**/*.ts\" && prettier --write \"server/**/*.ts\"",
        "start": "yarn serve",
        "start:server:dev": "tsc-watch -P server/tsconfig.build.json --onSuccess \"node dist/server-app/main.js\"",
        "start:debug": "tsc-watch -p server/tsconfig.build.json --onSuccess \"node --inspect-brk dist/server-app/main.js\"",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "deploy": "sh scripts/deploy.sh",
        "devtools": "ts-node -P server/tsconfig.json server/remote-devtools.ts"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.4",
        "@angular/common": "~8.2.4",
        "@angular/compiler": "~8.2.4",
        "@angular/core": "~8.2.4",
        "@angular/forms": "~8.2.4",
        "@angular/platform-browser": "~8.2.4",
        "@angular/platform-browser-dynamic": "~8.2.4",
        "@angular/platform-server": "~8.2.4",
        "@angular/pwa": "^0.803.5",
        "@angular/router": "~8.2.4",
        "@angular/service-worker": "~8.2.4",
        "@nestjs/common": "^6.0.0",
        "@nestjs/core": "^6.0.0",
        "@nestjs/ng-universal": "^2.0.1",
        "@nestjs/platform-express": "^6.0.0",
        "@nestjs/platform-socket.io": "^6.6.7",
        "@nestjs/platform-ws": "^6.6.7",
        "@nestjs/serve-static": "^1.0.1",
        "@nestjs/websockets": "^6.6.7",
        "@nguniversal/express-engine": "^8.0.0",
        "@nguniversal/module-map-ngfactory-loader": "^8.0.0",
        "@types/ws": "^6.0.3",
        "deep-equal": "^1.1.0",
        "dotenv": "^8.1.0",
        "immutable": "^4.0.0-rc.12",
        "iq180-logic": "^1.0.8",
        "monocle-ts": "^2.0.0",
        "ng-zorro-antd": "^8.1.2",
        "ngx-strongly-typed-forms": "^8.0.1",
        "redux": "^4.0.4",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^2.6.2",
        "rxjs": "~6.4.0",
        "tslib": "^1.10.0",
        "uuid": "^3.3.3",
        "uuidv4": "^5.0.1",
        "webpack-bundle-analyzer": "^3.4.1",
        "xstate": "^4.6.7",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.2",
        "@angular/cli": "~8.3.2",
        "@angular/compiler-cli": "~8.2.4",
        "@angular/language-service": "~8.2.4",
        "@nestjs/testing": "^6.0.0",
        "@types/dotenv": "^6.1.1",
        "@types/express": "4.16.1",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/jest": "^24.0.18",
        "@types/node": "11.13.4",
        "@types/socket.io": "^2.1.2",
        "@types/supertest": "2.0.7",
        "@types/uuid": "^3.4.5",
        "codelyzer": "^5.0.0",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "jest": "24.7.1",
        "karma": "~4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "nodemon": "^1.18.11",
        "prettier": "1.17.0",
        "protractor": "~5.4.0",
        "remote-redux-devtools": "^0.5.16",
        "remotedev-server": "^0.3.1",
        "supertest": "4.0.2",
        "ts-jest": "24.0.2",
        "ts-loader": "^5.2.0",
        "ts-node": "~7.0.0",
        "tsc-watch": "2.2.1",
        "tsconfig-paths": "3.8.0",
        "tslint": "~5.15.0",
        "typescript": "<3.6.0",
        "wait-on": "^3.2.0",
        "webpack-cli": "^3.1.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "server",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "globals": {
            "ts-jest": {
                "tsConfig": "tsconfig.spec.json"
            }
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "browser": {
        "http": false,
        "https": false,
        "net": false,
        "path": false,
        "stream": false,
        "tls": false,
        "vm": false,
        "os": false,
        "repl": false,
        "crypto": false,
        "fs": false,
        "child_process": false
    }
}
